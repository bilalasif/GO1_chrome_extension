{
  "name": "GO",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "128" : "Resources/images/icon_extension_active_128.png"
  },

  "browser_action": {
    "default_icon": {
      "19" : "Resources/images/icon_extension_inactive_19.png",
      "32" : "Resources/images/icon_extension_inactive_32.png"
    },
    "default_title": "GO"
  },

  "background": {
    "scripts": [
      "scripts/App.js"
    ]
  },
  "web_accessible_resources": [
    "node_modules/*",
    "html/*",
    "Resources/*"
  ],
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "<all_urls>"
  ]
}
